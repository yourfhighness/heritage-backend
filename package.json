{
  "name": "Heritage-Backend",
  "version": "0.1.0",
  "description": "heritage-backend-project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "kill": "Taskkill /IM node.exe /F",
    "server": "nodemon --exec babel-node ./src/index.js",
    "createtables": "cross-env NODE_ENV=development npx sequelize db:migrate",
    "inserttables": "cross-env NODE_ENV=development npx sequelize db:seed:all",
    "deletetables": "cross-env NODE_ENV=development npx sequelize db:migrate:undo:all",
    "start:with-no-migration": "cross-env NODE_ENV=production babel-node ./src/index.js",
    "start": "cross-env NODE_ENV=production npx sequelize-cli db:migrate && cross-env NODE_ENV=production npx sequelize-cli db:seed:all && cross-env NODE_ENV=production babel-node ./src/index.js"
  },
  "keywords": "HERITAGE",
  "author": "KEY JOSHUA",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "babel-core": "^6.26.3",
    "babel-install": "2.1.0",
    "babel-loader": "^8.0.6",
    "babel-node": "0.0.1-security",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.24.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "generate-sms-verification-code": "^1.0.5",
    "http-status": "^1.5.0",
    "joi": "^17.3.0",
    "joi-phone-number": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "prettier": "^2.2.1",
    "sequelize": "^5.21.5",
    "twilio": "^3.55.0"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
